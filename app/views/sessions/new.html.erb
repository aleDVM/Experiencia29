<%= form_with(model: @user, url: sessions_path) do |form| %>
<div class="container">

  <div class="form-group">
      <%= form.label :email %>
      <%= form.text_field :email, id: :user_email %>
    </div>

    <div class="form-group">
      <%= form.label :password %>
      <%= form.password_field :password, id: :user_password %>
    </div>

    <div class="actions">
      <%= form.submit 'Ingresar' %>
    </div>
  </div>
<% end%>
